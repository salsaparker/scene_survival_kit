<div class="row">
    <div class="top-offset-3">
        <div class="medium-12 columns text-center">
          <div class="panel clearfix">
            <p class="text-left">Graphic Artist Listing here.</p>
          </div>
        </div>
    </div>
</div>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @merch_artists.each do |merch_artist| %>
      <tr>
        <td><%= link_to 'Show', merch_artist %></td>
        <td><%= link_to 'Edit', edit_merch_artist_path(merch_artist) %></td>
        <td><%= link_to 'Destroy', merch_artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Merch artist', new_merch_artist_path %>
