<h1>Local Visual Artists</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Type</th>
      <th>Phone number</th>
      <th>Email address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visual_artists.each do |visual_artist| %>
      <tr>
        <td><%= visual_artist.first_name %></td>
        <td><%= visual_artist.last_name %></td>
        <td><%= visual_artist.type %></td>
        <td><%= visual_artist.phone_number %></td>
        <td><%= visual_artist.email_address %></td>
        <td><%= link_to 'Show', visual_artist %></td>
        <td><%= link_to 'Edit', edit_visual_artist_path(visual_artist) %></td>
        <td><%= link_to 'Destroy', visual_artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visual artist', new_visual_artist_path %>
